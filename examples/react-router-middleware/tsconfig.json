{
  "compilerOptions": {
    "jsx": "react-jsx",
    "lib": ["dom.iterable", "dom", "es2022"],
    "moduleResolution": "bundler",
    "module": "esnext",
    "noEmit": true,
    "rootDirs": [".", ".react-router/types"],
    "strict": true,
    "target": "es2022",
    "types": ["vite/client"],
    "verbatimModuleSyntax": true
  },
  "include": ["**/*", ".react-router/types/**/*"],
}
