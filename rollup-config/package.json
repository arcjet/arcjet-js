{
  "name": "@arcjet/rollup-config",
  "version": "1.0.0-alpha.2",
  "description": "Custom rollup config for Arcjet projects",
  "license": "Apache-2.0",
  "homepage": "https://arcjet.com",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arcjet/arcjet-js.git",
    "directory": "rollup-config"
  },
  "engines": {
    "node": ">=18"
  },
