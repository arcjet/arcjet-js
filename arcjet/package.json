{
  "name": "arcjet",
  "version": "1.0.0-alpha.2",
  "description": "Arcjet TypeScript and JavaScript SDK core",
  "license": "Apache-2.0",
  "homepage": "https://arcjet.com",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arcjet/arcjet-js.git",
    "directory": "arcjet"
  },
  "engines": {
    "node": ">=18"
  },
  "type": "module",
  "main": "./index.js",
  "types": "./index.d.ts",
  "files": [
    "LICENSE",
    "README.md",
    "*.js",
    "*.d.ts",
    "*.ts",
    "!*.config.js"
  ],
  "scripts": {
    "prepublishOnly": "npm run build",
    "build": "rollup --config rollup.config.js",
    "lint": "eslint .",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest"
  },
  "dependencies": {
    "@arcjet/analyze": "1.0.0-alpha.2",
    "@arcjet/logger": "1.0.0-alpha.2",
    "@arcjet/protocol": "1.0.0-alpha.2"
  },
  "devDependencies": {
    "@arcjet/eslint-config": "1.0.0-alpha.2",
    "@arcjet/rollup-config": "1.0.0-alpha.2",
    "@arcjet/tsconfig": "1.0.0-alpha.2",
    "@edge-runtime/jest-environment": "2.3.7",
    "@jest/globals": "29.7.0",
    "@types/node": "18.18.0",
    "jest": "29.7.0",
    "rollup": "4.6.1",
    "typescript": "5.3.2"
  },
  "publishConfig": {
    "access": "public",
    "tag": "latest"
  }
}
