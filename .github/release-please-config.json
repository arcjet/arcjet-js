      "skip-github-release": true
    },
    "nosecone-sveltekit": {
      "component": "@nosecone/sveltekit",
      "skip-github-release": true
    },
    "protocol": {
      "component": "@arcjet/protocol",
      "skip-github-release": true
    },
    "redact": {
      "component": "@arcjet/redact",
      "skip-github-release": true
    },
    "redact-wasm": {
      "component": "@arcjet/redact-wasm",
      "skip-github-release": true
    },
    "rollup-config": {
