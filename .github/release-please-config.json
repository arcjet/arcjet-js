    }
  },
  "plugins": [
    {
      "type": "node-workspace",
      "merge": false
    },
    {
      "type": "linked-versions",
      "groupName": "arcjet-js",
      "components": [
        "root",
        "@arcjet/analyze",
        "@arcjet/analyze-wasm",
        "arcjet",
        "@arcjet/astro",
        "@arcjet/bun",
        "@arcjet/deno",
